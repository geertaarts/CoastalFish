@Article{Moore2005,
  author        = {Moore, Dale a and Overton, Michael W and Chebel, Ricardo C and Truscott, Marla L and BonDurant, Robert H},
  title         = {Evaluation of factors that affect embryonic loss in dairy cattle.},
  journal       = {Journal of the American Veterinary Medical Association (JAVMA)},
  year          = {2005},
  volume        = {226},
  number        = {7},
  pages         = {1112--1118},
  __markedentry = {[jfieberg:6]},
  abstract      = {OBJECTIVE To identify potential risk factors for embryonic loss before 35 to 42 days of gestation in dairy cattle. DESIGN Prospective observational study. ANIMALS 381 cows. PROCEDURE Body condition score was determined at the time of artificial insemination (AI; day 0) and on days 20, 23, and 27 and between days 35 and 41; serum progesterone concentration was measured on days 0; 20 or 21; and 23, 24, or 25. Cows were excluded from analyses if day 0 serum progesterone concentration was {\textgreater} or = 1.0 ng/mL and classified as pregnant on day 23 if serum progesterone was {\textgreater} 1.5 ng/mL on day 20 or 21 and day 23, 24, or 25. Cows were examined via transrectal ultrasonography on day 27 or 28 and rectally palpated for pregnancy on days 35 to 41. RESULTS 39{\%} of cows that were pregnant on day 23 lost their embryo by day 27, and 18{\%} of cows that were pregnant on day 27 or 28 were not pregnant on days 35 to 41. Breeding a pregnant cow posed the greatest risk for embryonic loss at both time periods. Mean serum progesterone concentrations on day 21 or 22 and day 23, 24, or 25 were lowest for cows that lost an embryo between days 24 and 28. Cows with a linear somatic cell count score {\textgreater} 4.5 before AI were twice as likely to lose the embryo by 35 to 41 days, compared with cows with a score {\textless} 4.5. CONCLUSIONS AND CLINICAL RELEVANCE Results suggest that embryonic loss could be reduced by more accurate estrus detection, reducing mastitis, and strategies to improve progesterone concentration after breeding.},
  archiveprefix = {arXiv},
  arxivid       = {1132},
  doi           = {10.1146/annurev.ecolsys.l},
  eprint        = {1132},
  isbn          = {1543-592X},
  issn          = {0003-1488},
  keywords      = {abortion,animals,bovine,bovine complications,cattle,cattle blood,cattle physiology,cell count,cell count veterinary,female,mastitis,milk,milk cytology,pregnancy,progesterone,progesterone blood,prospective studies,risk factors,veterinary,veterinary epidemiology,veterinary etiology,veterinary prevention {\&} control},
  pmid          = {15825738},
}

