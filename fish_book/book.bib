@Article{Halvorsen2016,
  author        = {Halvorsen, Rune and Mazzoni, Sabrina and Dirksen, John Wirkola and N{\ae}sset, Erik and Gobakken, Terje and Ohlson, Mikael},
  title         = {How important are choice of model selection method and spatial autocorrelation of presence data for distribution modelling by MaxEnt?},
  journal       = {Ecological Modelling},
  year          = {2016},
  volume        = {328},
  pages         = {108--118},
  __markedentry = {[jfieberg:6]},
  abstract      = {In this paper we explore new options for model selection in the distribution modelling method MaxEnt, offered by the strict maximum likelihood explanation of the method. These options, which are collectively referred to as the 'alternative MaxEnt procedure' (aMp), imply manual forward stepwise selection of variables and an F-ratio test to compare nested models. We compared aMp models with models obtained by the default variable transformation and model selection procedures in the popular Maxent.jar software ('default MaxEnt procedure'; dMp), which invokes ℓ1-regularisation. We used swamp-forest presence in SE Norway as the target for our modelling. Eleven terrain variables derived from a LiDAR-based digital elevation map, making up three groups of internally correlated variables, were used as predictors in the models. An independently collected presence/absence 'test' data set was used for evaluation of models. Although dMp models were much more complex than aMp models, with up to 240 variables derived from the original 11, compared to a maximum number of derived variables in aMp models of 14, no significant difference in performance between dMp and aMp models was found. This indicates that the regularisation procedure is not the main reason for the good performance of MaxEnt (as implemented in Maxent.jar) in comparative studies. Advantages of the more flexible aMp procedure, which produces simpler models, are discussed. Effects of spatial autocorrelation in the response variable on modelling results were addressed by obtaining parallel MaxEnt models for two response variables that differ in the number of presence observations: the centroid of the 121 swamp forests and the 7175 grid cells, each 5 m × 5 m, in which swamp forest was recorded as present. Our results are not conclusive with respect to the effect of spatial autocorrelation in the response variable on the predictive performance of distribution models by MaxEnt.},
  doi           = {10.1016/j.ecolmodel.2016.02.021},
  isbn          = {03043800},
  issn          = {03043800},
  keywords      = {Independent evaluation data,MaxEnt, Model selection, Overfitting,Spatial autocorrelation,Swamp forest},
  pmid          = {6581453},
  publisher     = {Elsevier B.V.},
}

@Article{Koenig1999,
  author        = {Koenig, Walter D.},
  title         = {Spatial autocorrelation of ecological phenomena},
  journal       = {Trends in Ecology and Evolution},
  year          = {1999},
  volume        = {14},
  number        = {1},
  pages         = {22--26},
  __markedentry = {[jfieberg:6]},
  abstract      = {Ecological variables often fluctuate synchronously over wide geographical areas, a phenomenon known as spatial autocorrelation or spatial synchrony. Development of statistical approaches designed to test for spatial autocorrelation combined with the increasing accessibility of long-term, large-scale ecological datasets are now making it possible to document the patterns and understand the causes of spatial synchrony at scales that were previously intractable. These developments promise to foster significant future advances in understanding population regulation, metapopulation dynamics and other areas of population ecology.},
  archiveprefix = {arXiv},
